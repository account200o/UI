-- Scripts:

local function PIYGG_fake_script() -- namecolor.color effect script 
	local script = Instance.new('LocalScript', namecolor)

	local UIGradient = script.parent
	while true do
		UIGradient.Rotation += 1
		wait()
	end
end
coroutine.wrap(PIYGG_fake_script)()
local function KJVM_fake_script() -- Main.Drag UI Script 
	local script = Instance.new('LocalScript', Main)

	script.Parent.Draggable = true
	script.Parent.Active = true
end
coroutine.wrap(KJVM_fake_script)()
